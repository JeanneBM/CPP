// Ten program czêœciowo inicjuje tablicê.
#include <iostream>
using namespace std;

int main()
{
  const int SIZE = 7;
  int numbers[SIZE] = { 1, 2, 4, 8 }; // Zainicjowanie pierwszych czterech elementów

  cout << "Zawartoœæ tablicy:\n";
  for (int index = 0; index < SIZE; index++)
    cout << numbers[index] << " ";

  cout << endl;
  return 0;
}
