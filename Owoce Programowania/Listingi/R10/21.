// Ten program demonstruje użycie funkcji
// członkowskiej length() klasy string.
#include <iostream>
#include <string>
using namespace std;

int main()
{
  string town;

  cout << "Podaj nazwę Twojego miasta: ";
  cin >> town;
  cout << "Nazwa Twojego miasta składa się z " << town.length();
  cout << " znaków.\n";
  return 0;
}
