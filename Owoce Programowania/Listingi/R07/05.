// Ten program wykorzystuje tablicê do zapisania
// pierwszych dziesiêciu znaków alfabetu i wyœwietlenia
// ich kodów ASCII.
#include <iostream>
using namespace std;

int main()
{
  const int NUM_LETTERS = 10;
  char letters[NUM_LETTERS] = { 'A', 'B', 'C', 'D', 'E',
                                'F', 'G', 'H', 'I', 'J' };

  cout << "Znak" << "\t" << "Kod ASCII\n";
  cout << "----" << "\t" << "---------\n";
  for (int count = 0; count < NUM_LETTERS; count++)
  {
    cout << letters[count] << "\t";
    cout << static_cast<int>(letters[count]) << endl;
  }
  return 0;
}
