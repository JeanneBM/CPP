// Ten program wyświetla wartości prawdy i fałszu.
#include <iostream>
using namespace std;

int main()
{
   bool trueValue, falseValue;
   int x = 5, y = 10;
   
   trueValue = x < y;
   falseValue = y == x;
   
   cout << "Prawda to  " << trueValue << "." << endl;
   cout << "Fałsz to  " << falseValue << "." << endl;
   return 0;
}
