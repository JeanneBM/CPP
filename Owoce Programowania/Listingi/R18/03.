// Ten program demonstruje użycie funkcji insertNode().
#include <iostream>
#include "NumberList.h"
using namespace std;

int main()
{
  // Definicja obiektu NumberList
  NumberList list;

  // Dołączenie do listy kilku węzłów
  list.appendNode(2.5);
  list.appendNode(7.9);
  list.appendNode(12.6);

  // Wstawienie węzła w środku listy
  list.insertNode(10.5);

  // Wyświetlenie zawartości węzłów
  list.displayList();
  return 0;
}
