// Program wyświetla trzy wiersze liczb.
#include <iostream>
using namespace std;

int main()
{
   int num1 = 2897, num2 = 5,    num3 = 837,
       num4 = 34,   num5 = 7,    num6 = 1623,
       num7 = 390,  num8 = 3456, num9 = 12;

   // Wyświetlenie pierwszego wiersza
   cout << num1 << "  " << num2 << "  " << num3 << endl;

   // Wyświetlenie drugiego wiersza
   cout << num4 << "  " << num5 << "  " << num6 << endl;

   // Wyświetlenie trzeciego wiersza
   cout << num7 << "  " << num8 << "  " << num9 << endl;
   return 0;
}
