#include <iostream>
#include <string>
#include <array>
using namespace std;

int main()
{
  const int SIZE = 4;

  // Zapisanie imion w obiekcie typu array
  array<string, SIZE> names = { "Janek", "Asia", "Darek", "Karolina" };

  // Wy≈õwietlenie imion
  cout << "Lista imion:\n";
  for (auto it = names.begin(); it != names.end(); it++)
    cout << *it << endl;

  return 0;
}
