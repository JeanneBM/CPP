#include <iostream>
#include <string>
#include <array>
using namespace std;

int main()
{
  const int SIZE = 4;

  // Zapisanie imion w obiekcie typu array
  array<string, SIZE> names = { "Janek", "Asia", "Darek", "Karolina" };

  // Wy≈õwietlenie imion
  cout << "Lista imion:\n";
  for (int index = 0; index < names.size(); index++)
    cout << names[index] << endl;

  return 0;
}
