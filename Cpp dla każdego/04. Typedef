  /*definiowane poprzez typedef;
  pozwala między innymi nadać inną nazwę już istniejącemu typowi
  instrukcja:
  int wiek;
  jest równoważna:
  typedef int wiek;
  wiek twojWiek; //oznacza: int twojWiek;
  */
  
  #include <iostream>   
  
  typedef unsigned short int USHORT;   
  
  int main()   
  {  
  
  using std::cout;  
  using std::endl;  
  
  USHORT Width = 5;  
  USHORT Length;  
  Length = 10;  
  USHORT Area  = Width * Length; 
  cout << "Szerokosc:" << Width << "\n"; 
  cout << "Dlugosc: "  << Length << endl;  
  cout << "Obszar: " << Area <<endl;  
  
  return 0; 
      
  }
